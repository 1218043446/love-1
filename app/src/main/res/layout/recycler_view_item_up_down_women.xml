<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="12dp">


    <ImageView
        android:id="@+id/item_up_down_women_iv_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/icon_up_down_women" />

    <!--  android:background="@mipmap/icon_up_down_dia_women"-->
    <TextView
        android:id="@+id/item_up_down_women_tv_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:background="@drawable/icon_up_down_dia_women"
        android:gravity="center_vertical"
        android:paddingLeft="28dp"
        android:paddingRight="20dp"
        android:text="66"
        android:textColor="@color/black"
        android:textSize="@dimen/size_16"
        app:layout_constraintBottom_toBottomOf="@+id/item_up_down_women_iv_icon"
        app:layout_constraintLeft_toRightOf="@+id/item_up_down_women_iv_icon"
        app:layout_constraintTop_toTopOf="@+id/item_up_down_women_iv_icon" />

</android.support.constraint.ConstraintLayout>